<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppCognitive.Views.CognitiveMDP"
             xmlns:pages="clr-namespace:AppCognitive.Views">
  <MasterDetailPage.Master>
        <pages:CognitiveMDPMaster></pages:CognitiveMDPMaster>
  </MasterDetailPage.Master>
  <MasterDetailPage.Detail>
    <NavigationPage x:Name="Navigator"
        BarBackgroundColor="{StaticResource MainColor}">
      <x:Arguments>
        <pages:CognitiveMDPDetail></pages:CognitiveMDPDetail>
      </x:Arguments>
    </NavigationPage>
  </MasterDetailPage.Detail>
</MasterDetailPage>